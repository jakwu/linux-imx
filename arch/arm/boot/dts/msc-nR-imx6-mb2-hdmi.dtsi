/ {
	soc {
		aips-bus@02100000 { /* AIPS2 */
			i2cmux {
				i2c2@1 {
					hdmi_display {
						compatible = "fsl,imx6-hdmi-i2c";
						reg = <0x50>;
					};
				};
			};
		};
	};

	mxcfb1: fb@0 {
		compatible = "fsl,mxc_sdc_fb";
		disp_dev = "hdmi";
		interface_pix_fmt = "RGB24";
		mode_str ="1920x1080M-24@50";
		default_bpp = <24>;
		int_clk = <0>;
		late_init = <0>;
		status = "okay";
	};
};

&hdmi_core {
	ipu_id = <0>;
	disp_id = <0>;
	status = "okay";
};

&hdmi_video {
	fsl,phy_reg_vlev = <0x0294>;
	fsl,phy_reg_cksymtx = <0x800d>;
	status = "okay";
};
